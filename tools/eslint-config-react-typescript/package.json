{
  "name": "@lukeshay/eslint-config-react-typescript",
  "version": "0.0.1",
  "description": "My eslint configuration for react typescript.",
  "main": "index.js",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "scripts": {
    "format": "yarn lint --fix",
    "lint": "eslint --ext .ts,.tsx,.js,.jsx --ignore-path ../../.gitignore .",
    "build": "exit 0",
    "test": "exit 0",
    "test:coverage": "exit 0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/LukeShay/lukeshay-web.git"
  },
  "author": "Luke Shay <https://github.com/LukeShay>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/LukeShay/lukeshay-web/issues"
  },
  "homepage": "https://github.com/LukeShay/lukeshay-web#readme",
  "dependencies": {
    "@lukeshay/eslint-config-typescript": "^0.0.5",
    "eslint-plugin-react": "^7.19.0"
  },
  "peerDependencies": {
    "typescript": "^3.8.3"
  }
}
