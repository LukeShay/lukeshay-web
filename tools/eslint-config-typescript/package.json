    "test:coverage": "echo 'No unit tests defined'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/LukeShay/lukeshay-web.git"
  },
  "author": "Luke Shay <https://github.com/LukeShay>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/LukeShay/lukeshay-web/issues"
  },
  "homepage": "https://github.com/LukeShay/lukeshay-web#readme",
  "dependencies": {
    "@lukeshay/eslint-config": "0.0.1",
    "@typescript-eslint/parser": "^2.22.0",
    "@typescript-eslint/eslint-plugin": "^2.22.0"
  },
  "peerDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "typescript": "^3.8.3"
  }
}
