{
  "name": "@lukeshay/eslint-config-typescript",
  "version": "0.0.5",
  "description": "My eslint configuration for typescript.",
  "main": "index.js",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "scripts": {
    "build": "exit 0",
    "clean": "rm -rf .rush *.log",
    "format": "eslint --fix --ext .ts,.tsx,.js,.jsx --ignore-path ../../.gitignore .",
    "lint": "eslint --ext .ts,.tsx,.js,.jsx --ignore-path ../../.gitignore .",
    "test": "exit 0",
    "test:coverage": "exit 0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/LukeShay/lukeshay-web.git"
  },
  "author": "Luke Shay <https://github.com/LukeShay>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/LukeShay/lukeshay-web/issues"
  },
  "homepage": "https://github.com/LukeShay/lukeshay-web#readme",
  "dependencies": {
    "@lukeshay/eslint-config": "0.0.3",
    "@typescript-eslint/parser": "^2.22.0",
    "@typescript-eslint/eslint-plugin": "^2.22.0",
    "eslint": "^6.8.0"
  },
  "peerDependencies": {
    "typescript": "^3.8.3"
  }
}
