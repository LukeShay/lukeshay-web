{
  "name": "@lukeshay/rollup-config",
  "version": "0.0.2",
  "description": "My rollup configuration for JavaScript and TypeScript",
  "input": "index.js",
  "main": "dist/cjs/index.js",
  "cjs": "dist/cjs/index.js",
  "esm": "dist/es/index.js",
  "exports": {
    "require": "./dist/cjs/index.js",
    "import": "./dist/es/index.js"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "scripts": {
    "prebuild": "mkdir dist",
    "build": "rollup -c --silent",
    "clean": "rm -rf dist *.log",
    "format": "eslint . --ext .js --ignore-path ../../.gitignore --fix",
    "lint": "eslint . --ext .js --ignore-path ../../.gitignore",
    "test": "exit 0",
    "test:coverage": "exit 0"
  },
  "repository": "https://github.com/LukeShay/route-rating-web.git",
  "author": "Luke Shay <https://github.com/LukeShay>",
  "license": "MIT",
  "dependencies": {
    "rollup": "*",
    "@rollup/plugin-commonjs": "*",
    "@rollup/plugin-node-resolve": "*",
    "@rollup/plugin-strip": "*",
    "rollup-plugin-terser": "*",
    "rollup-plugin-typescript2": "*",
    "@rollup/plugin-auto-install": "*",
    "@rollup/plugin-json": "*"
  },
  "devDependencies": {
    "@lukeshay/eslint-config": "1.0.2",
    "prettier": "^2.0.5",
    "eslint": "^7.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.2"
  },
  "files": [
    "dist",
    "CHANGELOG.md",
    "README.md"
  ],
  "tags": [
    "rollup",
    "lukeshay",
    "javascript",
    "typescript"
  ]
}
