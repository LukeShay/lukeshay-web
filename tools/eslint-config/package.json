{
  "name": "@lukeshay/eslint-config",
  "version": "0.0.2",
  "description": "My eslint configuration",
  "main": "index.js",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "scripts": {
    "format": "yarn lint --fix",
    "lint": "eslint --ext .ts,.tsx,.js,.jsx --ignore-path ../../.gitignore .",
    "build": "exit 0",
    "test": "echo 'No unit tests defined'",
    "test:coverage": "echo 'No unit tests defined'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/LukeShay/lukeshay-web.git"
  },
  "author": "Luke Shay <https://github.com/LukeShay>",
  "license": "MIT",
  "dependencies": {
    "prettier": "^1.19.1"
  },
  "peerDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2"
  }
}
